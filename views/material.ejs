    <!-- Board Section -->
    <!-- Page Content -->
    <div class="container">
        <br><br><br>
        <div class="row">
            <div class="col-md-3">
                <p class="lead">Stages</p>
                <div class="list-group">
                    <a href="/material/1/1" class="list-group-item">Stage 1</a>
                    <a href="/material/2/1" class="list-group-item">Stage 2</a>
                    <a href="/material/3/1" class="list-group-item">Stage 3</a>
                </div>
            </div>
            
            <div class="col-md-9">
                <div class="upload">
                    <button type="button" class="btn" onclick="document.location.href='/material_board'">Upload</button>
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>no</th>
                                    <th>title</th>
                                </tr>
                            </thead>
                            <tbody>
                                <%
                                    for(var i=(page*page_num)-page_num; i<(page*page_num); i++){
                                        if(i>leng){
                                            i++;
                                        }else{
                                            var item=rows[i];
                                        %>
                                <tr>
                                    <td class="material_no" name="material_no"><%=item.material_no%></a></td>
                                    <td class="material_title" name="material_title"><%=item.material_title%></td>
                                </tr>
                                <%
                                    }
                                    }
                                    %>
                                <tr>
                                    <td colspan="2">
                                        <%
                                            for(var j=0;j<rows.length/10;j++){
                                                
                                            %>
                                        [<a href="/material/<%=stage%>/<%=j+1%>"><%=j+1%></a>]
                                        <%
                                            }
                                            %>
                                        </tr>
                            </tbody>

                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            
        </div>
        
    </div>
