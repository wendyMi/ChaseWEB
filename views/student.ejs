        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2">
                    <div class="modal-body">
                        <h2>Student List</h2>
                        <hr class="star-primary">
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th>Student_id</th>
                                            <th>Student_name</th>
                                            <th>Progress</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%
                                            for(var i=(page*page_num)-page_num; i<(page*page_num); i++){
                                                if(i>leng){
                                                    i++;
                                                }else{
                                                    var item=rows[i];
                                                %>
                                        <tr>
                                            <td class="user_id" name="user_id"><%=item.user_id%></td>
                                            <td class="student_name" name="student_name"><%=item.student_name%></a></td>
                                            <td class="user_done" name="user_process"><%=item.user_process%></a></td>
                                        </tr>
                                        <%
                                            }
                                            }
                                            %>
                                        <tr>
                                            <td colspan="3">
                                                <%
                                                    for(var j=0;j<rows.length/10;j++){
                                                        
                                                    %>
                                                [<a href="/fnq/<%=j+1%>"><%=j+1%></a>]
                                                <%
                                                    }
                                                    %>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
                        </div>
                        <!-- /.panel-body -->
                        <form name="sentMessage" id="contactForm" novalidate method="post">
                            <div class="row control-group">
                                <div class="form-group col-xs-12 controls">
                                    <div class="col-xs-2">
                                        <p>Student</p>
                                    </div>
                                    <div class="col-xs-4">
                                        <input type="text" class="form-control" placeholder="Student-ID" name="student_id">
